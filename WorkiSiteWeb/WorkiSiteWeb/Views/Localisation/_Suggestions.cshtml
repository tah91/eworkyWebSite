@model System.Collections.Generic.IEnumerable<Localisation>

@{ if (Model.Count() != 0)
   {
    <div class="contentBlock">
        <h3 class="blue">@Worki.Resources.Views.Localisation.LocalisationString.Suggestion</h3>
            <ul>
                @{
                   foreach (var item in Model)
                   {
                        <li><a class="preview" href="@item.GetDetailFullUrl(Url)">@item.Name</a></li>
                   }
                }
            </ul>
    </div>
    }
}