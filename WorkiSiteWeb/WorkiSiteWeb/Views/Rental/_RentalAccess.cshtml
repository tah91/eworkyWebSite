@model Worki.Data.Models.RentalAccess
@{
	var select = new SelectList(Rental.AccessTypes, "Key", "Value", Model.Type); 
  }

<div class="rentalAccessRow margint5">
	@using (Html.BeginCollectionItem("Rental.RentalAccesses"))
 {
	 <span class="editor-label-access">
		@Html.LabelFor(m => m.Type)
	 </span>
	 <span class="editor-field marginr10">
		@Html.DropDownListFor(m => m.Type,select)
	 </span>
	<span class="editor-label-access">
		@Html.LabelFor(m => m.Line)
	 </span>
	 <span class="editor-field marginr10">
		@Html.TextBoxFor(m => m.Line)
	 </span>
	 	<span class="editor-label-access">
		@Html.LabelFor(m => m.Station)
	 </span>
	 <span class="editor-field">
		@Html.TextBoxFor(m => m.Station)
	 </span>
    <button class="deleteRow crossButton cross"></button>
}
</div>