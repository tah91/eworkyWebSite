@model BookingListViewModel

<div class="contentBlock">
@if (!string.IsNullOrEmpty(Model.Title))
{ 
    @RazorHelpers.TitleDiv(Model.Title, "Blue")
}

@if (Model.Bookings.List.Count != 0)
{
    <ul>
    @foreach (var booking in Model.Bookings.List)
    {
        @Html.Partial(MVC.Backoffice.Shared.Views._BookingItem, booking)
    }
    </ul>
    <div class="pager">
        @Html.PageLinks(Model.Bookings.PagingInfo, x => Url.Action(MVC.Backoffice.Home.Booking(x)))
    </div>  
}
else
{
    <div class="emptyContent italic">
        @Html.Raw(string.Format(Worki.Resources.Views.Profile.ProfileString.AddedSpacesEmpy, Html.ActionLink(Worki.Resources.Views.Home.FaqString.ClickHere, MVC.Home.AddSpace())))
    </div>
}
</div>