@model BackofficeMenu

@Html.Partial(MVC.Shared.Views._HorizontalMenu, new List<MenuItem> 
{ 
	new MenuItem{ Selected = Model == BackofficeMenu.Home, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Home, MVC.Backoffice.Home.Index())} ,
	new MenuItem{ Selected = Model == BackofficeMenu.Clients, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Clients, MVC.Backoffice.Client.Clients())} ,
	new MenuItem{ Selected = Model == BackofficeMenu.Profil, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Options, MVC.Backoffice.Profil.ChangePaymentInformation())}
})