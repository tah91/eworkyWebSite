@model AdminMenu

@Html.Partial(MVC.Shared.Views._HorizontalMenu, new List<MenuItem> 
	{
        new MenuItem{ Selected = Model == AdminMenu.Sheet, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Sheet, MVC.Admin.Sheet.Index())} ,
		new MenuItem{ Selected = Model == AdminMenu.Member, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Members, MVC.Admin.Member.IndexUser())} ,
		new MenuItem{ Selected = Model == AdminMenu.Activity, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Activity, MVC.Admin.Activity.IndexWelcomePeople())} ,
		new MenuItem{ Selected = Model == AdminMenu.Moderation, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Moderation, MVC.Admin.Moderation.Stat())}
	})