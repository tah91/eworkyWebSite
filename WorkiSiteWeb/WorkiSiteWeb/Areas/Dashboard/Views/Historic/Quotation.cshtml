@model PagingList<MemberQuotation>

@section HeadContent
{
	<title>@Html.WorkiTitle(Worki.Resources.Menu.Menu.Quoations)</title>
	<link href="@Url.VersionedContent(Links.Content.Dashboard_min_css)" rel="stylesheet" type="text/css" />
}

@Html.Partial(MVC.Dashboard.Shared.Views._DashboardMenu, DashboardMenu.Historic)

<div class="detailContent">

	<div class="blockContainer float-left contentLeft">
		@Html.Partial(MVC.Shared.Views._VerticalMenu, new List<MenuItem> 
		{ 
			new MenuItem{ Link= Html.ActionLink(Worki.Resources.Menu.Menu.Bookings, MVC.Dashboard.Historic.Booking())} ,
            new MenuItem{ Selected = true, Link= Html.ActionLink(Worki.Resources.Menu.Menu.Quoations, MVC.Dashboard.Historic.Quotation())}
		})
	</div>

	<div class="blockContainer float-left contentRight">
		<div class="contentBlock">
            @Html.Partial(MVC.Dashboard.Shared.Views._QuotationList,
            new QuotationListViewModel {
                                        PageList = Model,
                                        EmptyMessage = Worki.Resources.Views.BackOffice.BackOfficeString.EmptyQuotationHistoric,
                                        UrlMaker = x => Url.Action(MVC.Dashboard.Historic.Quotation(x))
                                    })
		</div>
	</div>
	<div class="clear"></div>
</div>