      connectionString="data source=winsqleu06.1and1.fr,1433;Database=db369325114;Integrated Security=false;User ID=dbo369325114;Password=poiuyt84"
      xdt:Transform="Replace" xdt:Locator="Match(name)"/>
    <add name="WorkiDBConnectionString"
      connectionString="data source=winsqleu06.1and1.fr,1433;Database=db369325114;Integrated Security=false;User ID=dbo369325114;Password=poiuyt84"
